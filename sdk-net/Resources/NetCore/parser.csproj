    <RootNamespace>Hime.Generated</RootNamespace>
    <AssemblyName>Hime.Generated</AssemblyName>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Version>1.0.0</Version>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="*.bin" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Hime.Redist" Version="3.3.2"/>
  </ItemGroup>
  <PropertyGroup>
    <RestoreSources>$(RestoreSources);$(HimeLocalNuget)</RestoreSources>
  </PropertyGroup>
</Project>
