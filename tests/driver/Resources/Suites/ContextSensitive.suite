fixture ContextSensitive

test Test_CS_NoContext_LR:
	grammar Test_CS_NoContext_LR {
		options {Axiom="e";}
		terminals {
			X0 -> 'x';
			context inner1 { X1 -> 'x'; }
		}
		rules {
			sub1 -> '('! #inner1{ X1* } ')'!;
			e -> (X0 | sub1)* ;
		}
	}
	parser LALR1
	on "x"
	yields e(X0)

